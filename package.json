{
  "name": "cheesejs",
  "private": true,
  "type": "module",
  "version": "1.0.4",
  "productName": "CheeseJS",
  "description": "A modern JavaScript/TypeScript playground",
  "author": "Damien Monasterios <monasterio13septiembre@gmail.com>",
  "repository": "github:PixelDroid19/CheeseJs",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder ",
    "build:dist": "vite build",
    "build:all": "vite build && electron-builder -mwl ",
    "build:mac": "vite build && electron-builder  --mac ",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "preview": "vite preview",
    "test": "vitest",
    "publish": "electron-builder --publish always",
    "release": "electron-builder",
    "quality": "npm run lint && npm run format:check && npm run test",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "prerelease": "npm run quality && npm run audit"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
