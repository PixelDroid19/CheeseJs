    "json-cycle": "^1.5.0",
    "lucide-react": "^0.554.0",
    "luna-object-viewer": "^0.3.2",
    "path-browserify": "^1.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.5",
    "react-object-view": "^0.3.5",
