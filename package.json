{
  "name": "cheesejs",
  "private": true,
  "version": "1.1.0",
  "productName": "CheeseJS",
  "author": "Damien Monasterios <monasterio13septiembre@gmail.com>",
  "repository": "github:PixelDroid19/CheeseJs",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder ",
    "build:win": "vite build && electron-builder --win --x64 --publish never",
    "build:win:dir": "vite build && electron-builder --win --x64 --dir -c.directories.output=release/win-dir --publish never",
    "build:win:prepack": "vite build && pnpm exec electron-builder --prepackaged release/win-temp/win-unpacked --win --x64 --publish never",
    "build:dist": "vite build",
    "build:all": "vite build && electron-builder -mwl ",
    "build:mac": "vite build && electron-builder  --mac ",
    "lint": "eslint src electron --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src electron --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"electron/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"electron/**/*.{ts,js}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "bench": "vitest bench",
    "publish": "electron-builder --publish always",
    "release": "electron-builder",
    "quality": "npm run lint && npm run format:check && npm run type-check && npm run test",
    "doctor": "react-doctor .",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "prerelease": "npm run quality && npm run audit",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.46",
    "@ai-sdk/google": "^3.0.30",
    "@ai-sdk/openai": "^3.0.30",
    "@emotion/is-prop-valid": "^1.4.0",
    "@fontsource/courier-prime": "^5.2.8",
    "@fontsource/patrick-hand": "^5.2.8",
    "@lancedb/lancedb": "^0.26.2",
    "@swc/core": "^1.15.11",
    "@vscode/vscode-languagedetection": "^1.0.23",
    "@xenova/transformers": "^2.17.2",
    "ai": "^6.0.97",
    "cheerio": "^1.2.0",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.3",
    "framer-motion": "^12.34.3",
    "i18next": "^25.8.13",
    "i18next-browser-languagedetector": "^8.2.1",
    "json-cycle": "^1.5.0",
    "lucide-react": "^0.575.0",
    "mammoth": "^1.11.0",
    "path-browserify": "^1.0.1",
    "pdf-parse": "^2.4.5",
    "pyodide": "^0.29.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-json-view-lite": "^2.5.0",
    "react-split": "2.0.14",
    "rollup": "^4.58.0",
    "stringify-object": "^6.0.0",
    "tailwind-merge": "^3.5.0",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@babel/generator": "^7.29.1",
    "@babel/parser": "^7.29.0",
    "@babel/plugin-syntax-unicode-sets-regex": "^7.18.6",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/js": "^10.0.1",
    "@monaco-editor/react": "^4.7.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.2.0",
    "@tailwindcss/vite": "^4.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/babel__standalone": "^7.1.9",
    "@types/babel__traverse": "^7.28.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "eslint": "^10.0.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-security": "^4.0.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "monaco-editor": "^0.55.1",
    "playwright": "^1.58.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "react-doctor": "^0.0.29",
    "sass": "^1.97.3",
    "standard": "^17.1.2",
    "tailwindcss": "^4.2.0",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^4.0.18"
  },
  "main": "dist-electron/main.js",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.30.0+sha512.2b5753de015d480eeb88f5b5b61e0051f05b4301808a82ec8b840c9d2adf7748eb352c83f5c1593ca703ff1017295bc3fdd3119abb9686efc96b9fcb18200937"
}
