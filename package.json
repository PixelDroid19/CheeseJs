{
  "name": "cheesejs",
  "private": true,
  "version": "1.1.0",
  "productName": "CheeseJS",
  "author": "Damien Monasterios <monasterio13septiembre@gmail.com>",
  "repository": "github:PixelDroid19/CheeseJs",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder ",
    "build:win": "vite build && electron-builder --win --x64 --publish never",
    "build:win:dir": "vite build && electron-builder --win --x64 --dir -c.directories.output=release/win-dir --publish never",
    "build:win:prepack": "vite build && pnpm exec electron-builder --prepackaged release/win-temp/win-unpacked --win --x64 --publish never",
    "build:dist": "vite build",
    "build:all": "vite build && electron-builder -mwl ",
    "build:mac": "vite build && electron-builder  --mac ",
    "lint": "eslint src electron --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src electron --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"electron/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"electron/**/*.{ts,js}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "bench": "vitest bench",
    "publish": "electron-builder --publish always",
    "release": "electron-builder",
    "quality": "npm run lint && npm run format:check && npm run test",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "prerelease": "npm run quality && npm run audit",
    "prepare": "husky"
  },
  "dependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-proposal-decorators": "^7.28.6",
    "@babel/plugin-transform-async-generator-functions": "^7.28.6",
    "@babel/standalone": "^7.28.6",
    "@emotion/is-prop-valid": "^1.4.0",
    "@fontsource/courier-prime": "^5.2.8",
    "@fontsource/patrick-hand": "^5.2.8",
    "@swc/core": "^1.15.10",
    "@vscode/vscode-languagedetection": "^1.0.23",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.3",
    "framer-motion": "^12.28.1",
    "html2canvas": "^1.4.1",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "json-cycle": "^1.5.0",
    "lucide-react": "^0.562.0",
    "path-browserify": "^1.0.1",
    "pyodide": "^0.29.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "react-json-view-lite": "^2.5.0",
    "react-split": "2.0.14",
    "rollup": "^4.55.3",
    "stringify-object": "^6.0.0",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.9.3",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@babel/generator": "^7.28.6",
    "@babel/parser": "^7.28.6",
    "@babel/plugin-syntax-unicode-sets-regex": "^7.18.6",
    "@babel/traverse": "^7.28.6",
    "@babel/types": "^7.28.6",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@monaco-editor/react": "^4.7.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/babel__standalone": "^7.1.9",
    "@types/babel__traverse": "^7.28.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "monaco-editor": "^0.55.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "standard": "^17.1.2",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^4.0.17"
  },
  "main": "dist-electron/main.js",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  }
}
