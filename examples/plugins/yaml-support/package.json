{
  "name": "yaml-support",
  "displayName": "YAML Language Support",
  "version": "1.0.0",
  "description": "Adds YAML syntax highlighting and language features to CheeseJS",
  "author": {
    "name": "CheeseJS Team",
    "email": "plugins@cheesejs.dev"
  },
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "cheesejs": ">=1.0.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "yaml",
    "yml",
    "syntax",
    "highlighting"
  ],
  "contributes": {
    "languages": [
      {
        "id": "yaml",
        "aliases": [
          "YAML",
          "yaml"
        ],
        "extensions": [
          ".yaml",
          ".yml"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:yaml"
  ]
}
