{
  "$schema": "vscode://schemas/color-theme",
  "name": "Solarized Light",
  "type": "light",
  "colors": {
    "editor.background": "#fdf6e3",
    "editor.foreground": "#657b83",
    "editor.selectionBackground": "#eee8d5",
    "editor.lineHighlightBackground": "#eee8d5",
    "editorCursor.foreground": "#d30102",
    "editorWhitespace.foreground": "#eee8d5",
    "editorIndentGuide.background": "#eee8d5",
    "editorIndentGuide.activeBackground": "#93a1a1",
    "editorLineNumber.foreground": "#93a1a1",
    "editorLineNumber.activeForeground": "#657b83",

    "editorWidget.background": "#fdf6e3",
    "editorWidget.foreground": "#657b83",
    "editorWidget.border": "#eee8d5",

    "editorSuggestWidget.background": "#fdf6e3",
    "editorSuggestWidget.foreground": "#657b83",
    "editorSuggestWidget.border": "#eee8d5",
    "editorSuggestWidget.selectedBackground": "#eee8d5",
    "editorSuggestWidget.highlightForeground": "#268bd2",

    "editorHoverWidget.background": "#fdf6e3",
    "editorHoverWidget.foreground": "#657b83",
    "editorHoverWidget.border": "#eee8d5",

    "editor.findMatchBackground": "#073642",
    "editor.findMatchHighlightBackground": "#eee8d5",

    "editorError.foreground": "#dc322f",
    "editorWarning.foreground": "#cb4b16",
    "editorInfo.foreground": "#268bd2",

    "sideBar.background": "#eee8d5",
    "sideBar.foreground": "#657b83",
    "sideBar.border": "#93a1a1",

    "sideBarTitle.foreground": "#586e75",
    "sideBarSectionHeader.background": "#fdf6e3",
    "sideBarSectionHeader.foreground": "#586e75",

    "panel.background": "#fdf6e3",
    "panel.foreground": "#657b83",
    "panel.border": "#93a1a1",
    "panelTitle.activeBorder": "#268bd2",
    "panelTitle.activeForeground": "#586e75",
    "panelTitle.inactiveForeground": "#93a1a1",

    "titleBar.activeBackground": "#eee8d5",
    "titleBar.activeForeground": "#657b83",
    "titleBar.inactiveBackground": "#fdf6e3",
    "titleBar.inactiveForeground": "#93a1a1",
    "titleBar.border": "#93a1a1",

    "statusBar.background": "#eee8d5",
    "statusBar.foreground": "#657b83",
    "statusBar.border": "#93a1a1",
    "statusBar.noFolderBackground": "#eee8d5",
    "statusBar.debuggingBackground": "#859900",
    "statusBar.debuggingForeground": "#fdf6e3",

    "activityBar.background": "#fdf6e3",
    "activityBar.foreground": "#657b83",
    "activityBar.border": "#93a1a1",
    "activityBarBadge.background": "#268bd2",
    "activityBarBadge.foreground": "#fdf6e3",

    "tab.activeBackground": "#fdf6e3",
    "tab.activeForeground": "#002b36",
    "tab.inactiveBackground": "#eee8d5",
    "tab.inactiveForeground": "#93a1a1",
    "tab.border": "#93a1a1",
    "tab.activeBorder": "#268bd2",

    "input.background": "#eee8d5",
    "input.foreground": "#586e75",
    "input.border": "#93a1a1",
    "input.placeholderForeground": "#93a1a1",
    "inputOption.activeBorder": "#268bd2",
    "inputValidation.errorBackground": "#fdf6e3",
    "inputValidation.errorBorder": "#dc322f",
    "inputValidation.warningBackground": "#fdf6e3",
    "inputValidation.warningBorder": "#cb4b16",

    "button.background": "#268bd2",
    "button.foreground": "#fdf6e3",
    "button.hoverBackground": "#2aa198",
    "button.secondaryBackground": "#eee8d5",
    "button.secondaryForeground": "#657b83",
    "button.secondaryHoverBackground": "#93a1a1",

    "dropdown.background": "#fdf6e3",
    "dropdown.foreground": "#657b83",
    "dropdown.border": "#93a1a1",
    "dropdown.listBackground": "#fdf6e3",

    "list.activeSelectionBackground": "#eee8d5",
    "list.activeSelectionForeground": "#002b36",
    "list.hoverBackground": "#eee8d5",
    "list.hoverForeground": "#586e75",
    "list.inactiveSelectionBackground": "#eee8d5",
    "list.inactiveSelectionForeground": "#657b83",
    "list.highlightForeground": "#268bd2",

    "scrollbarSlider.background": "#93a1a180",
    "scrollbarSlider.hoverBackground": "#93a1a1b3",
    "scrollbarSlider.activeBackground": "#657b83",

    "badge.background": "#268bd2",
    "badge.foreground": "#fdf6e3",

    "terminal.background": "#fdf6e3",
    "terminal.foreground": "#657b83",
    "terminalCursor.foreground": "#d30102",
    "terminal.selectionBackground": "#eee8d5",
    "terminal.ansiBlack": "#073642",
    "terminal.ansiRed": "#dc322f",
    "terminal.ansiGreen": "#859900",
    "terminal.ansiYellow": "#b58900",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiMagenta": "#d33682",
    "terminal.ansiCyan": "#2aa198",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiBrightBlack": "#002b36",
    "terminal.ansiBrightRed": "#cb4b16",
    "terminal.ansiBrightGreen": "#586e75",
    "terminal.ansiBrightYellow": "#657b83",
    "terminal.ansiBrightBlue": "#839496",
    "terminal.ansiBrightMagenta": "#6c71c4",
    "terminal.ansiBrightCyan": "#93a1a1",
    "terminal.ansiBrightWhite": "#fdf6e3",

    "focusBorder": "#268bd2",
    "selection.background": "#eee8d5",
    "descriptionForeground": "#93a1a1",
    "foreground": "#657b83"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#93a1a1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control", "storage"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Keyword operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Storage type",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Function argument",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Library variable",
      "scope": ["support.other.variable"],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#657b83"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#dc322f",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#cb4b16",
        "fontStyle": "underline"
      }
    },
    {
      "name": "JSON String",
      "scope": ["meta.structure.dictionary.json string.quoted.double.json"],
      "settings": {
        "foreground": "#657b83"
      }
    },
    {
      "name": "Diff inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Diff deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "Diff changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Markup heading",
      "scope": ["markup.heading", "punctuation.definition.heading"],
      "settings": {
        "foreground": "#b58900",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Markup code",
      "scope": ["markup.raw", "markup.inline.raw"],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#657b83"
      }
    },
    {
      "name": "Regular expression",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "Escape character",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "Type annotation",
      "scope": ["meta.type.annotation", "support.type.primitive"],
      "settings": {
        "foreground": "#b58900"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.readonly": "#b58900",
    "property.readonly": "#b58900",
    "type": "#b58900",
    "class": "#b58900",
    "interface": "#b58900",
    "enum": "#b58900",
    "enumMember": "#cb4b16",
    "namespace": "#b58900",
    "function": "#268bd2",
    "method": "#268bd2",
    "parameter": "#cb4b16",
    "property": "#657b83",
    "variable": "#657b83"
  }
}
