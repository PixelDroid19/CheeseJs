{
  "name": "cheesejs-wasm-cpp",
  "version": "0.1.0",
  "description": "C++ language support for CheeseJS using WebAssembly",
  "displayName": "C++ (WASM)",
  "main": "index.js",
  "keywords": [
    "cheesejs",
    "plugin",
    "cpp",
    "wasm",
    "language"
  ],
  "contributes": {
    "wasmLanguages": [
      {
        "id": "cpp",
        "name": "C++",
        "extensions": [
          ".cpp",
          ".cc",
          ".cxx",
          ".h",
          ".hpp"
        ],
        "version": "17",
        "wasmPath": "runtime/runtime.wasm",
        "bindingsPath": "bindings/index.js",
        "monacoConfig": {
          "comments": {
            "lineComment": "//",
            "blockComment": [
              "/*",
              "*/"
            ]
          },
          "brackets": [
            [
              "{",
              "}"
            ],
            [
              "[",
              "]"
            ],
            [
              "(",
              ")"
            ]
          ],
          "autoClosingPairs": [
            {
              "open": "{",
              "close": "}"
            },
            {
              "open": "[",
              "close": "]"
            },
            {
              "open": "(",
              "close": ")"
            },
            {
              "open": "\"",
              "close": "\""
            },
            {
              "open": "'",
              "close": "'"
            },
            {
              "open": "<",
              "close": ">"
            }
          ],
          "surroundingPairs": [
            {
              "open": "{",
              "close": "}"
            },
            {
              "open": "[",
              "close": "]"
            },
            {
              "open": "(",
              "close": ")"
            },
            {
              "open": "\"",
              "close": "\""
            },
            {
              "open": "'",
              "close": "'"
            },
            {
              "open": "<",
              "close": ">"
            }
          ]
        }
      }
    ]
  },
  "permissions": [
    "storage"
  ],
  "sandboxed": true
}
